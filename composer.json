{
    "name": "orchestra/asset",
    "description": "Asset Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/asset/",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "asset"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Asset\\" : "src/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "qawemlilo/html": "^6.4",
        "orchestra/contracts": "^6.0",
        "orchestra/support-facades": "^6.0"
    },
    "require-dev": {
        "illuminate/filesystem": "^8.0",
        "nunomaduro/larastan": "^1.0.1",
        "orchestra/testbench": "^6.22"
    },
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/qawemlilo/html"
      }
    ],
    "suggest": {
        "illuminate/filesystem": "Allow using orchestra/asset component outside of Laravel app (^8.0).",
        "qawemlilo/html": "Allow using orchestra/asset with HTML component (^6.0)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
